{
  "name": "bitmasker",
  "version": "1.0.0",
  "description": "Utils for making and using bitmasks.",
  "main": "index.js",
  "bin": {
    "bitmasker": "cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm-run-all test:*",
    "test:cli-short-flags": "node bin/cli.js ./test/input.json -P parser -A contentTypes,extensions && node ./test/test.js",
    "test:cli-long-flags": "node bin/cli.js ./test/input.json --preferred-key=parser --alternate-keys=contentTypes,extensions && node ./test/test.js",
    "clean": "rm ./test/input.bitmasker.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ariutta/bitmasker.git"
  },
  "keywords": [
    "bitmask",
    "filter",
    "match"
  ],
  "author": "Anders Riutta",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ariutta/bitmasker/issues"
  },
  "homepage": "https://github.com/ariutta/bitmasker#readme",
  "dependencies": {
    "base58": "^0.1.0",
    "bitmask": "young-steveo/bitmask.js#345d7088bc2f52a87a0e02c2a89b5f40f03c1d9d",
    "bitwise": "^0.2.0",
    "commander": "^2.9.0",
    "fast-bitset": "^1.3.2",
    "funary": "^1.0.0",
    "js-crc": "^0.1.0",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "npm-run-all": "^3.0.0"
  }
}
